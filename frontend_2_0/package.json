{
  "name": "miRExplore",
  "version": "1.0.0",
  "description": "miRExplore Front End; Thankfully used https://github.com/computnik/express-react-ts-mui-boilerplate as tempalte",
  "main": "index.js",
  "scripts": {
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "webpack --mode production",
    "build": "concurrently 'build:server' 'build:client'",
    "start": "npm run build && node server",
    "dev:client": "webpack serve --mode development --devtool inline-source-map --hot",
    "dev:server": "nodemon",
    "dev": "concurrently 'npm:dev:server' 'npm:dev:client'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mjoppich/miRExplore.git"
  },
  "keywords": [
    "bioinformatics",
    "miRNA",
    "gene",
    "interaction"
  ],
  "author": "Markus Joppich <joppich@bio.ifi.lmu.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mjoppich/miRExplore/issues"
  },
  "homepage": "https://github.com/mjoppich/miRExplore#readme",
  "dependencies": {
