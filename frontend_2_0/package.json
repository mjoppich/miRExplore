    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@tsconfig/node12": "^1.0.9",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.27",
    "@types/react": "^18.0.7",
    "@types/react-dom": "^18.0.0",
    "@types/react-table": "^7.7.12",
