{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "sourceMap": true,
        "baseUrl": "./src",
        "allowSyntheticDefaultImports": true,
      },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "./node_modules/*"
        ]
}
